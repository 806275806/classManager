<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ljd.classmanager.Dao.CourseofClassDao">
    <insert id="insertCourseofClass" parameterType="ljd.classmanager.Entity.CourseOfClassEntity">
        insert into course_of_class(course_code,class_code,class_nickname,dept_id)
        values (#{courseCode},#{classCode},#{classNickname},#{deptId})
    </insert>
</mapper>